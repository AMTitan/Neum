/// Position Absolute

pos-a => {{{pos-absolute}}}
pos-absolute => position: absolute

/// Position Static

pos-stic => {{{pos-static}}}
pos-static => position: static

/// Position Relitive

pos-r => {{{pos-relitive}}}
pos-relitive => position: relitive

/// Position Fixed

pos-f => {{{pos-fixed}}}
pos-fixed => position: fixed

/// Position Sticky

pos-sty => {{{pos-sticky}}}
pos-sticky => position: sticky

/// Top

pt-{} => {{{ top-{}  }}}
top-{}% => top: {}%
top-{}px  => top: {}px
top-[{}] => top: {}

/// Bottom

pb-{} => {{{ bottom-{}  }}}
bottom-{}% => bottom: {}%
bottom-{}px  => bottom: {}px
bottom-[{}] => bottom: {}

/// Left

pl-{} => {{{ left-{}  }}}
left-{}% => left: {}%
left-{}px  => left: {}px
left-[{}] => left: {}

/// Right

pr-{} => {{{ right-{}  }}}
right-{}% => right: {}%
right-{}px  => right: {}px
right-[{}] => right: {}

/// Transform

transform-y-{} => -ms-transform: translateY({});transform: translateY({})
transform-x-{} => -ms-transform: translateX({});transform: translateX({})
transform-xy-{} => -ms-transform: translateX({}) translateY({});transform: translateX({}) translateY({})
